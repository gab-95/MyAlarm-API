package it.myalert.DTO;


import java.util.Date;

import javax.validation.constraints.Email;
import javax.validation.constraints.NotEmpty;

public class UserDTO {
	
	int id;
	String Nome;
	String Cognome;
	@Email @NotEmpty 
	String Email;
	String Password;
	String Citta;
	String Indirizzo;
	Date DataNascita;
	String Sesso;
	

}
